{
  "name": "jothsa-site",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint:style": "stylelint '**/*.{css,postcss,svelte}'",
    "lint:es": "eslint .",
    "lint:prettier": "prettier --cache --plugin prettier-plugin-svelte --check .",
    "lint": " pnpm lint:prettier; pnpm lint:es; pnpm lint:style",
    "format:style": "stylelint --fix '**/*.{css,postcss,svelte}'",
    "format:es": "eslint --fix",
    "format:prettier": "prettier --cache --plugin prettier-plugin-svelte --write .",
    "format": "pnpm format:prettier; pnpm format:es; pnpm format:style"
  },
  "devDependencies": {
    "@jothsa/stylelint-config": "^0.0.3",
    "@playwright/test": "^1.36.2",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/kit": "^1.22.3",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-svelte": "^2.32.4",
    "mdsvex": "^0.11.0",
    "open-props": "^1.5.10",
    "prettier": "3.0.0",
    "prettier-plugin-svelte": "^3.0.3",
    "remark-emoji": "^3.1.2",
    "remark-unwrap-images": "^3.0.1",
    "shiki-twoslash": "^3.1.2",
    "stylelint": "^15.10.2",
    "supports-css": "^0.1.5",
    "svelte": "^4.1.1",
    "svelte-check": "^3.4.6",
    "tslib": "^2.6.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.7",
    "vitest": "^0.33.0"
  },
  "type": "module",
  "dependencies": {
    "@fec/remark-a11y-emoji": "^3.1.0",
    "@sentry/sveltekit": "^7.60.1",
    "postcss": "^8.4.27",
    "postcss-preset-env": "^9.1.0",
    "shiki": "^0.14.3"
  }
}
