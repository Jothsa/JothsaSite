{
  "name": "jothsa-site",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint:style": "stylelint '**/*.{css,postcss,svelte}'",
    "lint:es": "eslint .",
    "lint:prettier": "prettier --cache --plugin prettier-plugin-svelte --check .",
    "lint": " pnpm lint:prettier; pnpm lint:es; pnpm lint:style",
    "format:style": "stylelint --fix '**/*.{css,postcss,svelte}'",
    "format:es": "eslint --fix",
    "format:prettier": "prettier --cache --plugin prettier-plugin-svelte --write .",
    "format": "pnpm format:prettier; pnpm format:es; pnpm format:style"
  },
  "devDependencies": {
    "@jothsa/stylelint-config": "^0.0.3",
    "@planetscale/database": "^1.11.0",
    "@playwright/test": "^1.37.1",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/adapter-netlify": "^2.0.8",
    "@sveltejs/kit": "^1.22.6",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@zerodevx/svelte-img": "^2.0.2",
    "csstype": "^3.1.2",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.32.4",
    "mdsvex": "^0.11.0",
    "open-props": "^1.5.13",
    "prettier": "3.0.2",
    "prettier-plugin-svelte": "^3.0.3",
    "remark-emoji": "^4.0.0",
    "remark-unwrap-images": "^3.0.1",
    "shiki-twoslash": "^3.1.2",
    "stylelint": "^15.10.3",
    "supports-css": "^0.1.5",
    "svelte": "^4.2.0",
    "svelte-check": "^3.5.0",
    "tslib": "^2.6.2",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vitest": "^0.34.2"
  },
  "dependencies": {
    "@fec/remark-a11y-emoji": "^3.1.0",
    "media-chrome": "^1.4.0",
    "postcss": "^8.4.28",
    "postcss-preset-env": "^9.1.1",
    "sharp": "^0.32.5",
    "shiki": "^0.14.3"
  }
}
